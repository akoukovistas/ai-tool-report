{
  "name": "ai-metrics-report",
  "version": "0.1.0",
  "description": "Node CLI for GitHub Copilot user enrichment and Cursor Admin API metrics.",
  "type": "module",
  "bin": {
    "ai-metrics-report": "bin/ai-metrics-report.js"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build": "echo 'No build step (pure ESM JS)'",
    "lint": "echo 'Add a linter (eslint) if desired'",
    "test": "node --test",
    "reports": "ai-metrics-report reports --help",
    "report:one-shot": "ai-metrics-report reports one-shot",
    "report:active-users": "ai-metrics-report reports active-users",
    "report:ai-tooling": "ai-metrics-report reports ai-tooling",
    "report:recent-activity": "ai-metrics-report reports recent-activity",
    "report:all": "ai-metrics-report reports all",
    "start": "node src/server/index.js",
    "dev": "node --watch src/server/index.js",
    "dashboard": "npm start"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "marked": "^16.3.0",
    "node-cron": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.17.0"
  }
}
